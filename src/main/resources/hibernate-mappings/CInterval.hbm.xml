<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.lucia.trucksmanagement.persistence.domain.CInterval" table="c_interval">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="native" />
        </id>
        <property name="timeIni" column="time_ini" not-null="true"/>
        <property name="timeFin" column="time_fin" />
        <property name="carrierId" column="carrier_id" />
        
     </class>
</hibernate-mapping>
